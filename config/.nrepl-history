(require 'clojure.edn)
(def mappings (clojure.edn/read-string (slurp "elasticsearch-mappings.edn")))
(def mappingstr (slurp "elasticsearch-mappings.edn"))
(set-env! :dependencies '[[org.clojure/data.json "0.2.6"]])
(require '[clojure.data.json :as json])
(doc json/read-string)
(doc json/read-str)
(json/read-str mappingstr)
(spit "mapp.edn" (json/read-str mappingstr))
(require 'clojure.set)
(require 'clojure.walk)
(clojure.walk/keywordize-keys (json/read-str mappingstr))
(spit "mapping.edn" (clojure.walk/keywordize-keys (json/read-str mappingstr)))
(clojure.edn/read-string "mapping.edn")
(clojure.edn/read-string (slurp "mapping.edn"))
 (slurp "mapping.edn")
(clojure.edn/read (clojure.java.io/input-stream "mapping.edn"))
(clojure.edn/read (clojure.java.io/reader "mapping.edn"))
(doc clojure.edn/read)
(clojure.edn/read (clojure.java.io/input-stream "mapping.edn"))
 (slurp "mapping.edn")
(clojure.edn/read-string (slurp "mapping.edn"))
(spit "mapping.edn" (prn-str (clojure.walk/keywordize-keys (json/read-str mappingstr))))
(clojure.edn/read-string (slurp "mapping.edn"))
exit
